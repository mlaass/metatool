<template name="sol2-lua">
	<member name="serialize" delim=", " remove_last_delim="True">
		  &quot;{SHORT_MEMBER_NAME}&quot;, &amp;{MEMBER_NAME})</member>
	<struct name="meta" delim="">
		lua.new_usertype&lt;{TYPE}&gt;(&quot;{TYPE}&quot;,{members.serialize}
		);
	</struct>
<file name="header" ext="hpp">
	/** autogenerated file **/
	#ifndef {INCLUDE_GUARD_NAME}
	#define {INCLUDE_GUARD_NAME}

	#include &lt;sol/sol.hpp&gt;
	/*{INCLUDES}*/
	void register_lua_types(sol::state &amp;lua){
		{structs.meta}
	};
	#endif {INCLUDE_GUARD_NAME}
</file>
</template>
