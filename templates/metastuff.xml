<template name="metastuff">
	<member name="serialize" delim=", " remove_last_delim="True">
		 member(&quot;{SHORT_MEMBER_NAME}&quot;, &amp;{MEMBER_NAME})
	</member>
	<struct name="meta">
		inline auto registerMembers&lt;{TYPE}&gt;() {{
		return members(
		{members.serialize}
		)}};
	</struct>
	<file name="header" ext="h">
		/** autogenerated file **/
		#ifndef {INCLUDE_GUARD_NAME}
		#define {INCLUDE_GUARD_NAME}

		#include &lt;Meta.h&gt;
		{INCLUDES}

		namespace meta {{
		{structs.meta}
		}}
		#endif {INCLUDE_GUARD_NAME}
	</file>
</template>
