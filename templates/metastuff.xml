<template name="metastuff">
	<member name="serialize" delim=", " remove_last_delim="True"> member(&quot;{SHORT_MEMBER_NAME}&quot;, &amp;{MEMBER_NAME})</member>
	<method name="serialize" delim=", " remove_last_delim="True"> method&lt;{MEMBER_TYPE}&gt;(&quot;{SHORT_MEMBER_NAME}&quot;, {MEMBER_NAME}) </method>
	<function name="serialize" delim=", " remove_last_delim="True"> function&lt;{FUNCTION_TYPE}&gt;(&quot;{FUNCTION_NAME}&quot;, {FUNCTION_NAME}) </function>
	<struct name="meta" delim="">
		inline auto registerMembers&lt;{TYPE}&gt;() {{
		return members(
		{members.serialize}
		{methods.serialize}
		{functions.serialize}
		)}};
	</struct>
	<file name="header" ext="h">
		/** autogenerated file **/
		#ifndef {INCLUDE_GUARD_NAME}
		#define {INCLUDE_GUARD_NAME}

		#include &lt;Meta.h&gt;
		{INCLUDES}

		namespace meta {{
		{structs.meta}
		}}
		#endif {INCLUDE_GUARD_NAME}
	</file>
</template>
