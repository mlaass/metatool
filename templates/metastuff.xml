<template name="metastuff">
	<member name="serialize" delim=", " remove_last_delim="True">member(&quot;{SHORT_NAME}&quot;, &amp;{NAME})</member>
	<method name="serialize" delim=", " remove_last_delim="True">method&lt;{TYPE}&gt;(&quot;{SHORT_NAME}&quot;, {NAME})</method>
	<function name="serialize" delim=", " remove_last_delim="True">function&lt;{TYPE}&gt;(&quot;{NAME}&quot;, {NAME})
	</function>
	<struct name="meta" delim="">
		inline auto registerMembers&lt;{TYPE}&gt;() {{
		return members(
		{members.serialize}
		{methods.serialize}
		)}};
	</struct>
	<file name="header" ext="h">
		/** autogenerated file **/
		#ifndef {INCLUDE_GUARD_NAME}
		#define {INCLUDE_GUARD_NAME}

		#include &lt;Meta.h&gt;
		{INCLUDES}

		namespace meta {{
		{structs.meta}
		}}

		{functions.serialize}
		#endif {INCLUDE_GUARD_NAME}
	</file>
</template>
